{
  "name": "terrarium",
  "homepage": "https://vctorahernndez.github.io/terrarium-vite/",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts}'",
    "format": "prettier --write './src/**/*.{js,ts,css,json}' --config ./.prettierrc",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "vite"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@types/papaparse": "5.3.15",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "gh-pages": "6.3.0",
    "prettier": "3.0.3",
    "typescript": "5.8.2",
    "vite": "6.0.5"
  },
  "dependencies": {
    "3d-tiles-renderer": "0.4.6",
    "dotenv": "16.4.7",
    "papaparse": "5.5.1",
    "three": "0.172.0",
    "yargs": "17.7.2"
  }
}
